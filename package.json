    "c8": "^10.1.3",
    "chai": "^6.2.0",
    "coveralls": "^3.1.1",
    "markdownlint-cli": "^0.45.0",
    "mocha": "^11.7.4",
    "mocha-steps": "^1.3.0",
    "shx": "^0.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "biome": "biome",
    "biome:check": "npm run biome -- check",
    "biome:fix": "npm run biome -- check --fix",
    "biome:migrate": "npm run biome -- migrate --write",
    "compile": "tsc --pretty --project .",
    "clean": "npm run clean:compile && npm run clean:coverage",
    "clean:compile": "shx rm -rf lib",
    "clean:coverage": "shx rm -rf coverage .nyc_output",
